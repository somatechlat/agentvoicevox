    {
      "alias": "github",
      "displayName": "GitHub",
      "providerId": "github",
      "enabled": false,
      "updateProfileFirstLoginMode": "on",
      "trustEmail": true,
      "storeToken": false,
      "addReadTokenRoleOnCreate": false,
      "authenticateByDefault": false,
      "linkOnly": false,
